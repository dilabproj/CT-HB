[flake8]
; E501 line too long (80 > 79 characters)
; E731 do not assign a lambda expression, use a def
; E402 module import not at top; same as pylint wrong-import-position, use pylint instead
; W503 line break before binary operator; mutually exclusive with check W504
ignore = E501,E731,E402,W503
show-source = True
exclude = .venv,.tox,dist,doc,build,*.egg,ansible/*,.git/*
